{
    "name": "keboola/ex-generic-v2",
    "license": "MIT",
    "type": "project",
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "keboola/juicer": "^4.10.1",
        "keboola/php-utils": "^2.2",
        "keboola/filter": "^1.1",
        "symfony/config": "^5.2",
        "symfony/console": "^5.2",
        "symfony/finder": "^5.2",
        "symfony/process": "^5.2",
        "keboola/php-temp": "^1.0",
        "guzzlehttp/oauth-subscriber": "^0.2",
        "guzzlehttp/cache-subscriber": "dev-master#aaab195",
        "guzzlehttp/retry-subscriber": "dev-master#270393c",
        "symfony/validator": "^5.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "keboola/coding-standard": ">=9.0",
        "php-parallel-lint/php-parallel-lint": "^1.2",
        "phpstan/phpstan": "^0.12"
    },
    "autoload": {
        "psr-4": { "Keboola\\GenericExtractor\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Keboola\\GenericExtractor\\Tests\\": "tests/phpunit" }
    },
    "scripts": {
        "tests-phpunit": "phpunit",
        "tests": [
            "@tests-phpunit"
        ],
        "phpstan": "phpstan analyse ./src ./tests --level=max --no-progress -c phpstan.neon",
        "phpcs": "phpcs -n --ignore=vendor --extensions=php .",
        "phpcbf": "phpcbf -n --ignore=vendor --extensions=php .",
        "phplint": "parallel-lint -j 10 --exclude vendor .",
        "build": [
            "@phplint",
            "@phpcs",
            "@phpstan",
            "@tests"
        ],
        "ci": [
            "@composer validate --no-check-publish --no-check-all",
            "@build"
        ]
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
